{
  "name": "draft",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "electron-dev": "run-p electron-dev:*",
    "electron-dev:vite": "vite -c vite-electron.config.js",
    "electron-dev:electron": "electron src/background.js",
    "electron-build": "run-s electron-build:*",
    "electron-build:vite": "vite build -c vite-electron.config.js",
    "electron-build:electron": "electron-builder build",
    "postinstall": "node node_modules/electron/install.js && node node_modules/esbuild/install.js"
  },
  "dependencies": {
    "@crhio/jsdraft": "../",
    "@popperjs/core": "^2.9.2",
    "chokidar": "^3.5.1",
    "codemirror": "^5.60.0",
    "electron-context-menu": "^2.5.0",
    "electron-window-state": "^5.0.3",
    "is-electron": "^2.2.0",
    "jszip": "^3.6.0",
    "lodash": "^4.17.21",
    "mousetrap": "^1.6.5",
    "nanoid": "^3.1.22",
    "prettier": "^2.3.0",
    "vue": "^3.2.21",
    "vuex": "^4.0.2",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.1.4",
    "@vue/compiler-sfc": "^3.2.21",
    "electron": "^11.3.0",
    "electron-builder": "^22.9.1",
    "electron-reloader": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.32.8",
    "vite": "^2.0.0",
    "vite-svg-loader": "^1.5.1"
  },
  "main": "src/background.js",
  "build": {
    "directories": {
      "output": "build"
    },
    "mac": {
      "target": "dmg"
    },
    "files": [
      "src/background.js",
      "src/electron",
      "dist",
      "package.json"
    ]
  }
}
